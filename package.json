{
	"name": "defiant",
	"description": "Defiant - Chrome/Chromium Browser Extension for increased Privacy",
	"private": true,
	"version": "2021.07",
	"scripts": {
		"clean": "node ./defiant/make.mjs clean",
		"build": "node ./defiant/make.mjs build",
		"pack": "node ./defiant/make.mjs pack",
		"test": "mkdir /tmp/defiant; chromium --user-data-dir=/tmp/defiant --load-extension=$PWD/defiant"
	},
	"dependencies": {
		"esbuild": "^0.14.23"
	}
}
